{% extends "AppPanel/Layouts/base.html.twig" %}
  {% block javascripts %}
      {{ parent() }}

  {% endblock %}
{% block metaTag %}
    {{ parent() }}
    <meta name="description" content="مجله زیبایی صبا ریکو مقالات و اخرین اخبار در مورد متد های جدید آرایشگری و مدلینگ و سبک زندگی متدهای جدید رنگ مو اکستنشن مژه هایلایت مو اکستنشن مو کاشت ناخن و ابرو">
    <meta name="keywords" content="مجبه زیبا صبا ریکو-صبا ریکو-آموزش آرایشگری-مقالات اموزشی زیبایی و سلامت - فیلم های اموز آرایشگری">
    <link rel="canonical" href="https://sabarico.com/aboutUs" />
{% endblock %}
{% block body %}
    {{ parent() }}
    {{ render(controller('App\\Controller\\AppPanel\\PanelController::headerTopAction')) }}

    {{ render(controller('App\\Controller\\AppPanel\\PanelMenuController::menuAction')) }}
    <div class="container mt-15">
        <div class="row  text-right flex-direction-r">
            <div class="col-12 card-details m-15-990 ">
                <div class="card border mrl-10 ">
                    <div class="card-header bg-navyBlue fontSize09em">
                        <div>
                            <div class="d-flex  justify-content-between flexDirection alignItemsCenter">
                                <div class="">درباره ما</div>
                            </div>
                        </div>
                    </div>
                    <amp-img
                            alt="ق"
                            title="ق"
                            src="{{ absolute_url(asset('uploads/mainpagepics/about-us-sabaRico-com.jpg' ))}}"
                            width="400"
                            height="150"
                            aria-describedby="ق"
                            layout="responsive"></amp-img>
                    <div class="card-body p-10 lineHeight25em">
                        <h3>مجله زیبایی صبا ریکو</h3>
                        <p>مجله زیبایی صبا ریکو یک مجله زیبایی است که سعی دارد متد های جدید و صحیح آرایشگری و زیبایی را در اختیار اقایان و خانم های عزیز قرار دهد. این آموزش های در قالب مطالب و مقالات و اخبار تهیه و در مجله منتشر میشود.</p>
                        <p>این مطالب آموزشی شامل مدل های مختلف رنگ مو یا طرح های جدید شینوین مو متد های آرایشی جدید,نقد و بررسی سالن های زیبایی و معرفی ارایشگرانی که در این حوزه فعالیت میکنند میباشد.</p>
                        <p>بر خلاف سایر وب سایت ها مطالب این سایت صرفا کپی سایت های دیگر با کمی ویرایش نیست .این مطالب حاصل تجربه مستقیم ارایشگران به نام در سطح شهر تهران است و اخبار این مجله شامل و تالیف سایت های معتبری است که در حوزه زیبایی و سلامت فعالیت میکنند</p>
                    </div>
                </div>
            </div>

        </div>

    </div>




    <amp-animation id="hideAnim"
                   layout="nodisplay">
        <script type="application/json">
            {
                "duration": "200ms",
                "fill": "both",
                "iterations": "1",
                "direction": "alternate",
                "animations": [{
                    "selector": "#scrollToTopButton",
                    "keyframes": [{
                        "opacity": "0",
                        "visibility": "hidden"
                    }]
                }]
            }
        </script>
    </amp-animation>
    <amp-animation id="showAnim"
                   layout="nodisplay">
        <script type="application/json">
            {
                "duration": "200ms",
                "fill": "both",
                "iterations": "1",
                "direction": "alternate",
                "animations": [{
                    "selector": "#scrollToTopButton",
                    "keyframes": [{
                        "opacity": "1",
                        "visibility": "visible"
                    }]
                }]
            }
        </script>
    </amp-animation>

    <div class="container pt-40 pb-40">
        <div class="d-flex mr-10 justify-content-between flexDirection alignItemsCenter">
            <div class="btnOutInfo p-rl-15-450">
                <strong class="text-dark">گالری عکس</strong>
            </div>

        </div>
        <div class="row alignItemsCenter p-15">

            <amp-carousel height="300" layout="fixed-height" type="carousel" lightbox id="carousel-with-lightbox">
                <amp-img src="https://www.sabarico.com/uploads/images/extension-eyelash/extension-eyelash-sabaRico-2019-2.jpg" width="400" height="300" title="دوره آموزشی اکستنشن مژه" alt="نمونه کار های اکستشن مژه صبا شعبانی"></amp-img>
                <amp-img src="https://www.sabarico.com/uploads/images/extension-eyelash/extension-eyelash-sabaRico-2019.jpg" width="400" height="300"  title="دوره آموزشی اکستنشن مژه" alt="صبا شعبانی آموزش اکستشن مژه در تهران "></amp-img>
                <amp-img src="https://www.sabarico.com/uploads/images/extension-eyelash/extension-eyelash-valium-1.jpg" width="400" height="300" title="آموزش اکستشن مژه در تهران " alt="آموزش اکستشن مژه در تهران "></amp-img>
                <amp-img src="https://www.sabarico.com/uploads/images/hairDyeHighlight/hair-dye-highlight-sabaShabani.webp" width="400" height="300" title="صبا شعبانی رنگ و های لایت مو" alt="صبا شعبانی رنگ و های لایت مو"></amp-img>
                <amp-img src="https://www.sabarico.com/uploads/images/hairDyeHighlight/hair-dye-highlight-sabaShabani-1.webp" width="400" height="300" title="آموزش رنگ و هایلایت مو " alt="آموزش رنگ و هایلایت مو "></amp-img>
                <amp-img src="https://www.sabarico.com/uploads/images/hairDyeHighlight/hair-dye-highlight-sabaShabani-3.webp" width="400" height="300" title="اموزش رنگ مو هایلایت مو" alt="اموزش رنگ مو هایلایت مو"></amp-img>
                <amp-img src="https://www.sabarico.com/uploads/images/hairDyeHighlight/hair-dye-highlight-sabaShabani-12.webp" width="400" height="300" title="کلاس آموزش های لایت مو" alt="کلاس آموزش های لایت مو"></amp-img>
            </amp-carousel>



        </div>
    </div>
    {{ render(controller('App\\Controller\\AppPanel\\PanelController::footerAction' )) }}

{% endblock %}