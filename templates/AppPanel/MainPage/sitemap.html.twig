<?xml version="1.0" encoding="UTF-8"?>
<urlset
        xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9
            http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">
    <!-- created with Free Online Sitemap Generator www.xml-sitemaps.com -->

    <url>
        <loc>{{ url('mainPageIndex') }}</loc>
        <lastmod>2019-06-01</lastmod>
        <priority>1.00</priority>
    </url>
    <url>
        <loc>{{ url('aboutUS') }}</loc>
        <lastmod>2019-07-01</lastmod>
        <priority>1.00</priority>
    </url>
    <url>
        <loc>{{ url('ServiceList') }}</loc>
        <lastmod>2019-06-01</lastmod>
        <priority>1.00</priority>
    </url>
    <url>
        <loc>{{ url('NewsList') }}</loc>
        <lastmod>2019-06-01</lastmod>
        <priority>1.00</priority>
    </url>
    <url>
        <loc>{{ url('CategoryNewsList') }}</loc>
        <lastmod>2019-06-01</lastmod>
        <priority>1.00</priority>
    </url>
    <url>
        <loc>{{ url('ServiceMain') }}</loc>
        <lastmod>2019-06-01</lastmod>
        <priority>1.00</priority>
    </url>
    <url>
        <loc>{{ url('ArticleList') }}</loc>
        <lastmod>2019-06-02</lastmod>
        <priority>1.00</priority>
    </url>
    <url>
        <loc>{{ url('CategoryArticleList') }}</loc>
        <lastmod>2019-06-02</lastmod>
        <priority>1.00</priority>
    </url>




    {% for articel in ArticleData %}
        <url>
            <loc>{{ url('ArticleDetails',{'id': articel.id,'slug':articel.urlSlug}) }}</loc>
            <lastmod>{{ articel.dateInsert|date("Y-m-d") }}</lastmod>
            <priority>1.00</priority>
        </url>
    {% endfor %}
    {% for News in Newss %}
        <url>
            <loc>
                {{ url('NewsDetailsIndex',{'id': News.id,'slug':News.urlSlug}) }}
            </loc>
            <lastmod>{{ News.dateInsert|date("Y-m-d") }}</lastmod>
            <priority>1.00</priority>
        </url>
    {% endfor %}

    {% for CategoryArticle in CategoryArticles %}
        <url>
            <loc>{{ url('ArticleCategoryDetails',{'id': CategoryArticle.id,'slug':CategoryArticle.urlSlug}) }}</loc>
            <lastmod>{{ CategoryArticle.dateInsert|date("Y-m-d") }}</lastmod>
            <priority>1.00</priority>
            <changefreq>weekly</changefreq>
        </url>
    {% endfor %}
    {% for CatServices in CategoryServices %}
        <url>
            <loc>{{ url('ServiceCategoryDetails',{'id': CatServices.id,'slug':CatServices.urlSlug}) }}</loc>
            <lastmod>{{ CatServices.dateInsert|date("Y-m-d") }}</lastmod>
            <priority>1.00</priority>
            <changefreq>weekly</changefreq>
        </url>
    {% endfor %}
    {% for Servic in Service %}
        <url>
            <loc>{{ url('ServiceDetailAction',{'id': Servic.id,'slug':Servic.urlSlug}) }}</loc>
            <lastmod>{{ Servic.dateInsert|date("Y-m-d") }}</lastmod>
            <priority>1.00</priority>
            <changefreq>weekly</changefreq>
        </url>
    {% endfor %}
    {% for Content in Contents %}
        <url>
            <loc>{{ url('ContentDetails',{'id': Content.id,'slug':Content.urlSlug}) }}</loc>
            <lastmod>{{ Content.dateInsert|date("Y-m-d") }}</lastmod>
            <priority>1.00</priority>
            <changefreq>weekly</changefreq>
        </url>
    {% endfor %}



</urlset>
